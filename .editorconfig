[*.cs]

# SA0001: XML comment analysis is disabled due to project configuration
dotnet_diagnostic.SA0001.severity = silent

# SA1614: Element parameter documentation should have text
dotnet_diagnostic.SA1614.severity = silent

# SA1616: Element return value documentation should have text
dotnet_diagnostic.SA1616.severity = silent

# IDE0017: Simplify object initialization
dotnet_style_object_initializer = true:silent

# SA1600: Elements should be documented
dotnet_diagnostic.SA1600.severity = silent

# SA1210: Using directives should be ordered alphabetically by namespace
dotnet_diagnostic.SA1210.severity = silent

# SA1201: Elements should appear in the correct order
dotnet_diagnostic.SA1201.severity = silent

# SA1401: Fields should be private
dotnet_diagnostic.SA1401.severity = none

# CS0169: The field 'ResponseEntity.oK' is never used
dotnet_diagnostic.CS0169.severity = silent

# SA1633: File should have header
dotnet_diagnostic.SA1633.severity = silent

# IDE0060: Remove unused parameter
dotnet_code_quality_unused_parameters = all:silent

# IDE0065: Misplaced using directive
csharp_using_directive_placement = outside_namespace:silent

# SA1512: Single-line comments should not be followed by blank line
dotnet_diagnostic.SA1512.severity = none

# SA1611: Element parameters should be documented
dotnet_diagnostic.SA1611.severity = silent

# SA1617: Void return value should not be documented
dotnet_diagnostic.SA1617.severity = silent

# CS0472: The result of the expression is always the same since a value of this type is never equal to 'null'
dotnet_diagnostic.CS0472.severity = none

# SA1121: Use built-in type alias
dotnet_diagnostic.SA1121.severity = silent
